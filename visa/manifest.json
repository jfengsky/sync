{
  "manifest_version": 2,
  "name": "visa 助手",
  "description": "携程 visa 助手",
  "version": "1.0.1",
  "background": {
    "scripts": ["jquery-2.1.3.min.js", "background.js"]
  },
  "content_scripts": [{
    "matches": ["https://ceac.state.gov/*"],
    "js": [
      "jquery-2.1.3.min.js",
      "global.js",
      "page/personal1.js",
      "page/personal2.js"
    ]
  }],
  "browser_action": {
    "default_icon": "visa_19_19.png",
    "default_popup": "popup.html"
  },
  "permissions": ["tabs", "http://*/*", "https://*/*"]
}